<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client"
	xmlns:i="urn:import:config.i18n.client" xmlns:common="urn:import:org.roda.wui.client.common"
	xmlns:search="urn:import:org.roda.wui.client.search" xmlns:wcag="urn:import:org.roda.wui.common.client.widgets.wcag"
	xmlns:d="urn:import:com.google.gwt.user.datepicker.client">

	<ui:style>
	</ui:style>

	<ui:with field='messages' type='config.i18n.client.ClientMessages' />

	<g:FlowPanel>
		<g:Label addStyleNames="label associative-label" ui:field="itemName"></g:Label>
		<g:Anchor addStyleNames="pull-right toolbarLink toolbarLinkSmall" ui:field="removeDynamicTextBoxButton"></g:Anchor>
		<g:Button addStyleNames="search-field-dialog" ui:field="dialogButton"><i class="fa fa-search"></i></g:Button>
		<g:TextBox title="Removable text box" addStyleNames="form-textbox" ui:field="hiddenItemId"></g:TextBox>
	</g:FlowPanel>
</ui:UiBinder>
